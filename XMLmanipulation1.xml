<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<!-- Include -> only method included will run -->

<suite name="Include keyword">
  <test thread-count="5" name="Include keyword">
    <classes>
      <class name="testNGbasics.DataDrivenTesting">
      	<methods>
      		<include name="negativeSearchFunctionalityTest"/>
      	</methods>
      </class>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
